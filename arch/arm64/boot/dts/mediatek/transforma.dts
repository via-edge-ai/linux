		vbus-supply = <&usb_p1_vbus>;
		status = "okay";

		usb_con: connector {
			compatible = "usb-c-connector";
			label = "USB-C";
			//data-role = "dual";
			//power-role = "dual";
			//try-power-role = "sink";
			data-role = "host";
			power-role = "source";
